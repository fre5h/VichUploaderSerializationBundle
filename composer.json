{
    "name": "fresh/vich-uploader-serialization-bundle",
    "description": "Provides integration between VichUploaderBundle and JMSSerializerBundle.",
    "keywords": ["bundle", "symfony", "VichUploaderBundle", "JMSSerializerBundle", "serialization", "file upload", "upload", "integration"],
    "homepage": "https://github.com/fre5h/VichUploaderSerializationBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Artem Henvald",
            "email": "genvaldartem@gmail.com",
            "role": "Creator"
        },
        {
            "name": "Community",
            "homepage": "https://github.com/fre5h/VichUploaderSerializationBundle/graphs/contributors",
            "role": "Contributors"
        }
    ],
    "support": {
        "email": "genvaldartem@gmail.com",
        "issues": "https://github.com/fre5h/VichUploaderSerializationBundle/issues"
    },
    "require": {
        "php": ">=7.2.5",
        "doctrine/common": "^2.11|^3.0",
        "doctrine/persistence": "^1.3|^2.0",
        "jms/serializer-bundle": "^3.7",
        "monolog/monolog": "^2.0",
        "symfony/config": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/property-access": "^5.0",
        "symfony/routing": "^5.0",
        "vich/uploader-bundle": "^1.16"
    },
    "require-dev": {
        "escapestudios/symfony2-coding-standard": "^3.11",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-doctrine":"^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpunit/phpunit": "^8.5",
        "symfony/yaml": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Fresh\\VichUploaderSerializationBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Fresh\\VichUploaderSerializationBundle\\Tests\\": "Tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
