{
    "name": "fresh/vich-uploader-serialization-bundle",
    "description": "Provides integration between VichUploaderBundle and JMSSerializerBundle.",
    "keywords": ["bundle", "symfony", "VichUploaderBundle", "JMSSerializerBundle", "serialization", "file upload", "upload", "integration"],
    "homepage": "https://github.com/fre5h/VichUploaderSerializationBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Artem Henvald",
            "email": "genvaldartem@gmail.com",
            "role": "Creator"
        },
        {
            "name": "Community",
            "homepage": "https://github.com/fre5h/VichUploaderSerializationBundle/graphs/contributors",
            "role": "Contributors"
        }
    ],
    "support": {
        "email": "genvaldartem@gmail.com",
        "issues": "https://github.com/fre5h/VichUploaderSerializationBundle/issues"
    },
    "require": {
        "php": ">=8.0.0",
        "doctrine/cache": "^2.1",
        "doctrine/common": "^3.2",
        "doctrine/persistence": "2.2",
        "jms/serializer-bundle": "^5.0",
        "monolog/monolog": "^2.3",
        "symfony/config": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/property-access": "^6.0",
        "symfony/routing": "^6.0",
        "vich/uploader-bundle": "^1.18"
    },
    "require-dev": {
        "escapestudios/symfony2-coding-standard": "^3.12",
        "phpstan/phpstan": "^1.2",
        "phpstan/phpstan-doctrine": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-symfony": "^1.0",
        "phpunit/phpunit": "^9.5",
        "symfony/yaml": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Fresh\\VichUploaderSerializationBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Fresh\\VichUploaderSerializationBundle\\Tests\\": "Tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
